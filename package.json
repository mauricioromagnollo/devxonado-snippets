{
  "name": "vscode-snippets",
  "displayName": "Xonado Snippets",
  "description": "Visual Studio Code snippets by x0n4d0",
  "publisher": "x0n4d0",
  "version": "1.2.4",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1b64b3",
    "theme": "dark"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/x0n4d0/vscode-snippets.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Snippets"
  ],
  "scripts": {
    "test": "npx vsce package && code --install-extension"
  },
  "devDependencies": {
    "vsce": "2.15.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": "shellscript",
        "path": "./snippets/bash.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescriptreact.json"
      }
    ]
  }
}
